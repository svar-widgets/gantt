{
	"name": "@svar-ui/svelte-gantt",
	"version": "2.4.2",
	"description": "Interactive and customizable Svelte Gantt chart component",
	"productTag": "gantt",
	"productTrial": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:dist": "vite build --mode dist",
		"build:tests": "vite build --mode test",
		"lint": "yarn eslint ./demos ./src",
		"start": "vite --open",
		"start:tests": "vite --open=/tests/ --host 0.0.0.0  --port 5100 --mode test",
		"test": "true",
		"test:cypress": "cypress run -P ./ --config \"baseUrl=http://localhost:5100/tests\""
	},
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js",
			"types": "./types/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"license": "GPL-3.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/svar-widgets/gantt.git"
	},
	"bugs": {
		"url": "https://forum.svar.dev"
	},
	"homepage": "https://svar.dev/svelte/gantt/",
	"dependencies": {
		"@svar-ui/gantt-data-provider": "2.4.2",
		"@svar-ui/gantt-locales": "2.4.2",
		"@svar-ui/gantt-store": "2.4.2",
		"@svar-ui/lib-dom": "0.11.1",
		"@svar-ui/lib-state": "1.9.6",
		"@svar-ui/lib-svelte": "0.5.2",
		"@svar-ui/svelte-core": "2.4.0",
		"@svar-ui/svelte-menu": "2.4.0",
		"@svar-ui/svelte-grid": "2.4.0",
		"@svar-ui/svelte-toolbar": "2.4.0",
		"@svar-ui/svelte-editor": "2.4.0"
	},
	"files": [
		"src",
		"types",
		"licenses",
		"readme.md",
		"whatsnew.md",
		"license.txt"
	],
	"keywords": [
		"svelte",
		"gantt",
		"gantt-chart",
		"svelte-gantt",
		"svelte-gantt-chart",
		"gantt chart",
		"gantt-chart-component",
		"gantt-diagram",
		"svelte-gantt-component",
		"svar widgets",
		"svar gantt",
		"timeline"
	]
}
